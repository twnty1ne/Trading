                    <positiveDecision/>
                </positiveNode>
                <negativeNode>
                    <negativeDecision/>
                </negativeNode>
            </splitNode>
        </positiveNode>
        <negativeNode>
            <negativeDecision/>
        </negativeNode>
    </splitNode>
</DecisionTree>